{
  "Kestrel": {
      "Endpoints": {
          "Http": {
              "Url": "http://0.0.0.0:5000"
          },
          "Https": {
              "Url": "https://0.0.0.0:5001"
          }
      },
      "EndpointDefaults": {
          "Url": "https://0.0.0.0:5001",
          "Protocols": "Http1"
      }
  },
  "Serilog": {
      "Using": [
          "Serilog.Sinks.Console",
          "Serilog.Sinks.Seq"
      ],
      "MinimumLevel": {
          "Default": "Verbose"
      },
      "Enrich": [
          "FromLogContext",
          "WithMachineName",
          "WithThreadId"
      ],
      "WriteTo": [
          {
              "Name": "Console"
          },
          {
              "Name": "Seq"
          }
      ]
  }
}