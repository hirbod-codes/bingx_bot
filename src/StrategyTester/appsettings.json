{
    "Serilog": {
        "Using": [
            "Serilog.Sinks.Console",
            "Serilog.Sinks.Seq"
        ],
        "MinimumLevel": {
            "Default": "Information"
        },
        "Enrich": [
            "FromLogContext",
            "WithMachineName",
            "WithThreadId"
        ],
        "WriteTo": [
            {
                "Name": "Console"
            },
            {
                "Name": "Seq",
                "Args": {
                    "serverUrl": "http://localhost:8086"
                }
            }
        ]
    },
    "BotName": "General",
    "BotOptions": {
        "General": {}
    },
    "BrokerName": "InMemory",
    "BrokerOptions": {
        "InMemory": {
            "BrokerCommission": 0.0005,
            "Symbol": "BTC-USDT",
            "AccountOptions": {
                "Balance": 10000
            }
        }
    },
    "MessageStoreName": "InMemory",
    "MessageStoreOptions": {
        "InMemory": {}
    },
    "NotifierName": "InMemory",
    "StrategyName": "General",
    "StrategyOptions": {
        "General": {
            "StrategyProvider": "providers.src.Providers.SmmaRsiStrategyProvider"
        }
    },
    "IndicatorsOptionsName": "SmmaRsi",
    "IndicatorsOptions": {
        "SmmaRsi": {
            "Smma1": {
                "Period": 65,
                "Source": "close"
            },
            "Smma2": {
                "Period": 85,
                "Source": "close"
            },
            "Smma3": {
                "Period": 100,
                "Source": "close"
            },
            "Rsi": {
                "Period": 14,
                "Source": "close",
                "UpperBand": 55,
                "LowerBand": 45
            }
        }
    },
    "RiskManagementName": "SmmaRsi",
    "RiskManagementOptions": {
        "SmmaRsi": {
            "Margin": 100,
            "SLPercentages": 50,
            "NumberOfConcurrentPositions": 0,
            "GrossLossLimit": 0,
            "GrossLossInterval": 0,
            "GrossProfitLimit": 0,
            "GrossProfitInterval": 0
        }
    },
    "PositionRepositoryType": "InMemory",
    "CandleRepositoryType": "InMemory",
    "MessageRepositoryType": "InMemory",
    "TesterName": "InMemory"
}
