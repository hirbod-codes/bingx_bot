{
    "BotName": "General",
    "StrategyName": "General",
    "MessageStoreName": "InMemory",
    "BrokerName": "InMemory",
    "StrategyOptions": {
        "General": {
            "StrategyProvider": "providers.src.Providers.SmmaRsiStrategyProvider"
        }
    },
    "MessageStoreOptions": {},
    "BrokerOptions": {
        "InMemory": {
            "BrokerCommission": 0.0005,
            "Symbol": "BTC-USDT",
            "AccountOptions": {
                "Balance": 10000
            }
        }
    },
    "IndicatorsOptions": {
        "Smma1": {
            "Period": 5,
            "Source": "close"
        },
        "Smma2": {
            "Period": 20,
            "Source": "close"
        },
        "Smma3": {
            "Period": 50,
            "Source": "close"
        },
        "Rsi": {
            "Period": 14,
            "Source": "close",
            "UpperBand": 55,
            "LowerBand": 45
        }
    },
    "RiskManagementOptions": {
        "Margin": 100,
        "Leverage": 60,
        "Ratio": 0.25,
        "SLPercentages": 50
    },
    "Serilog": {
        "Using": [
            "Serilog.Sinks.Console",
            "Serilog.Sinks.Seq"
        ],
        "MinimumLevel": {
            "Default": "Information"
        },
        "Enrich": [
            "FromLogContext",
            "WithMachineName",
            "WithThreadId"
        ],
        "WriteTo": [
            {
                "Name": "Console"
            },
            {
                "Name": "Seq",
                "Args": {
                    "serverUrl": "http://localhost:8086"
                }
            }
        ]
    }
}
